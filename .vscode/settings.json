{
  "cSpell.enableFiletypes": [
    "vue"
  ],
  "cSpell.ignoreWords": [
    "autoplay",
    "concat",
    "dcloudio",
    "getuserinfo",
    "ifdef",
    "ifndef",
    "nvue",
    "readonly",
    "scrolltolower",
    "swiper"
  ],
  "editor.formatOnType": true,
  "editor.tabSize": 2,
  "files.associations": {
    "*.json": "jsonc",
    "*.nvue": "vue"
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/src/pages.json"
      ],
      "url": "./.vscode/pages.schema.json"
    },
    {
      "fileMatch": [
        "/src/manifest.json"
      ],
      "url": "./.vscode/manifest.schema.json"
    }
  ],
  "path-autocomplete.pathMappings": {
    "/": "${folder}/src/"
  },
  "vetur.format.options.tabSize": 2,
  "vetur.format.options.useTabs": false,
  "vetur.format.scriptInitialIndent": true,
  "vetur.format.styleInitialIndent": true
}