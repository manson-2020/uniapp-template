{
	"pages": [
		{
			"path": "pages/index"
		},
		{
			"path": "pages/common/webview"
		},
		{
			"path": "pages/common/notFound",
			"style": {
				"navigationBarTitleText": "Oops!",
				"navigationStyle": "custom",
				"navigationBarTextStyle": "black",
				"disableScroll": true
			}
		},
		{
			"path": "pages/tabs/home",
			"style": {
				"navigationBarTitleText": "Home"
			}
		},
		{
			"path": "pages/tabs/mine",
			"style": {
				"navigationBarTitleText": "Mine"
			}
		},
		{
			"path": "pages/auth/login",
			"style": {
				"navigationBarTitleText": "Login",
				"navigationStyle": "custom",
				"topWindow": false,
				"leftWindow": false
			}
		},
		{
			"path": "pages/auth/register",
			"style": {
				"navigationBarTitleText": "Login",
				"navigationStyle": "custom",
				"topWindow": false,
				"leftWindow": false
			}
		}
	],
	"topWindow": {
		"path": "windows/top",
		"style": {
			"height": "60px"
		},
		"matchMedia": {
			"minWidth": 0
		}
	},
	"leftWindow": {
		"path": "windows/left",
		"style": {
			"width": "240px"
		}
	},
	"tabBar": {
		"color": "#7a7e83",
		"selectedColor": "#83a4d4",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/tabs/home",
				"iconPath": "static/img/tab-home.png",
				"selectedIconPath": "static/img/tab-home_.png",
				"text": "Home"
			},
			{
				"pagePath": "pages/tabs/mine",
				"iconPath": "static/img/tab-mine.png",
				"selectedIconPath": "static/img/tab-mine_.png",
				"text": "Mine"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#f5f5f5"
	},
	"easycom": {
		"autoscan": true
	}
}