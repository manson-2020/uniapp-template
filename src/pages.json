{
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#f5f5f5",
		"topWindow": false,
		"leftWindow": false
	},
	"pages": [
		{
			"path": "pages/index",
			"style": {
				"leftWindow": false,
				"topWindow": false
			}
		},
		{
			"path": "pages/tabs/home",
			"style": {
				"navigationBarTitleText": "Home",
				"leftWindow": false,
				"topWindow": false
			}
		},
		{
			"path": "pages/tabs/mine",
			"style": {
				"navigationBarTitleText": "Mine",
				"leftWindow": false,
				"topWindow": false
			}
		}
	],
	"subPackages": [
		{
			"root": "pages/common",
			"pages": [
				{
					"path": "webview",
					"style": {
						"topWindow": false,
						"leftWindow": false
					}
				},
				{
					"path": "upgrade",
					"style": {
						"app-plus": {
							"animationDuration": 200,
							"animationType": "fade-in",
							"background": "transparent",
							"backgroundColorTop": "transparent",
							"popGesture": "none",
							"scrollIndicator": false,
							"titleNView": false
						},
						"disableScroll": true
					}
				},
				{
					"path": "error",
					"style": {
						"navigationBarTitleText": "Oops, An error has occurred!",
						"navigationStyle": "custom",
						"navigationBarTextStyle": "black",
						"disableScroll": true,
						"topWindow": false,
						"leftWindow": false
					}
				}
			]
		},
		{
			"root": "pages/auth",
			"pages": [
				{
					"path": "applets",
					"style": {
						"navigationBarTitleText": "Auth",
						"leftWindow": false,
						"topWindow": false
					}
				},
				{
					"path": "login",
					"style": {
						"navigationBarTitleText": "Login",
						"navigationStyle": "custom",
						"topWindow": false,
						"leftWindow": false
					}
				},
				{
					"path": "register",
					"style": {
						"navigationBarTitleText": "Register",
						"navigationStyle": "custom",
						"topWindow": false,
						"leftWindow": false
					}
				},
				{
					"path": "reset",
					"style": {
						"navigationBarTitleText": "Reset",
						"navigationStyle": "custom",
						"topWindow": false,
						"leftWindow": false
					}
				}
			]
		},
		{
			"root": "pages/client",
			"pages": [
				{
					"path": "index"
				},
				{
					"path": "masonry"
				},
				{
					"path": "download"
				}
			]
		},
		{
			"root": "pages/admin",
			"pages": [
				{
					"path": "index",
					"style": {
						"navigationBarTitleText": "Index",
						"navigationStyle": "custom"
					}
				},
				{
					"path": "index1",
					"style": {
						"navigationBarTitleText": "Index1",
						"navigationStyle": "custom"
					}
				},
				{
					"path": "index2",
					"style": {
						"navigationBarTitleText": "Index2",
						"navigationStyle": "custom"
					}
				},
				{
					"path": "index3",
					"style": {
						"navigationBarTitleText": "Index3",
						"navigationStyle": "custom"
					}
				},
				{
					"path": "index4",
					"style": {
						"navigationBarTitleText": "Index4",
						"navigationStyle": "custom"
					}
				},
				{
					"path": "index5",
					"style": {
						"navigationBarTitleText": "Index5",
						"navigationStyle": "custom"
					}
				},
				{
					"path": "index6",
					"style": {
						"navigationBarTitleText": "Index6",
						"navigationStyle": "custom"
					}
				}
			]
		}
	],
	"topWindow": {
		"path": "windows/top",
		"style": {
			"height": "60px"
		},
		"matchMedia": {
			"minWidth": 0
		}
	},
	"leftWindow": {
		"path": "windows/left",
		"style": {
			"width": "240px"
		}
	},
	"tabBar": {
		"color": "#c1c1ca",
		"fontSize": "11px",
		"selectedColor": "#f45455",
		"backgroundColor": "#f5f5f5",
		"position": "bottom",
		"borderStyle": "black",
		"list": [
			{
				"pagePath": "pages/tabs/home",
				"iconPath": "static/img/tab-home_0.png",
				"selectedIconPath": "static/img/tab-home_1.png",
				"text": "Home"
			},
			{
				"pagePath": "pages/tabs/mine",
				"iconPath": "static/img/tab-mine_0.png",
				"selectedIconPath": "static/img/tab-mine_1.png",
				"text": "Mine"
			}
		]
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	}
}